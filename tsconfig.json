{ 
    "compilerOptions": {
      "target": "es2015",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "removeComments": false,
      "noImplicitAny": true,
      "suppressImplicitAnyIndexErrors": true,
      "esModuleInterop": true
    },
    "typedocOptions": {
      "inputFiles": [
        "./src/fwd/"
        // "./src/ts/fwd/runner/FwdWebRunner/FwdWebRunner.ts",
        // "./src/ts/fwd/runner/FwdWebRunner/FwdWebImpl.ts",
        // "./src/ts/fwd/runner/FwdWebRunner/StyleInjector.ts",

        // "./src/ts/fwd/runner/FwdWebRunner/components/AudioMeter.ts",
        // "./src/ts/fwd/runner/FwdWebRunner/components/BindableButton.ts",
        // "./src/ts/fwd/runner/FwdWebRunner/components/BindableController.ts",
        // "./src/ts/fwd/runner/FwdWebRunner/components/BindableSlider.ts",
        // "./src/ts/fwd/runner/FwdWebRunner/components/ConfigurationPanel.ts",
        // "./src/ts/fwd/runner/FwdWebRunner/components/Console.ts",
        // "./src/ts/fwd/runner/FwdWebRunner/components/MasterSlider.ts",
        // "./src/ts/fwd/runner/FwdWebRunner/components/Overlay.ts"
      ],
      "mode": "module",
      "out": "doc",
      "name": "Fwd API documentation",
      "includeVersion": true,
      "excludePrivate": true,
      "excludeNotExported": false,
      "exclude": "**/*+(index|.spec|.e2e|.test).ts"
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [ "node_modules" ] 
  }
  